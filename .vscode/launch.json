{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Bender",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/bender/main.go",
      "envFile": "${workspaceFolder}/deployments/dev/.env",
      "output": "${workspaceFolder}/bin/debug/bender"
    },
    {
      "name": "Launch Bender",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/cmd/bender/main.go",
      "envFile": "${workspaceFolder}/deployments/dev/.env",
      "output": "${workspaceFolder}/bin/debug/bender"
    }
  ]
}